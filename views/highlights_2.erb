	<link rel="stylesheet" href="style.css">
	<link href='http://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css'>

	<style>
		body { background-color: black; }
		.section { 
			display: block; 
			width: 300px; 
			height: 168.75px; 
			float: left; 
			margin: 0px; 
			opacity: 1.0; 
			border-radius: 0px; 
			/*top: 400px;*/
		}
		.blue { background-color: #06F; }
		.green { background-color: #0f9d00; }
		.red { background-color: #F00; }
		.yellow { background-color: #ffea00; }

		.text_section {
			padding-left: 10px;
		}
		p {
			font-family: 'Open Sans', sans-serif; 
			font-size: 16px;
			color: white; opacity: 0.9;
		}
		#container { width: 1280px; margin-top: -190px; margin-left: 120px;}  /*   container width = 712px * number of pictures  */
		#text_container{ width: 1280px;}

		a:link {text-decoration:none;}    /* unvisited link */
      	a:visited {text-decoration:none; color: #FFFFFF;} /* visited link */
      	a:active {text-decoration:underline;}  /* selected link */
      	a:link {text-decoration:none; color:#FFFFFF !important; opacity:0.8;}
      	a:hover {color:#FF0000 !important;} 

	</style>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>

</head>
